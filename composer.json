{
    "name": "originphp/generate",
    "description": "OriginPHP Generate Plugin. Generates code using boilerplate templates ready for use.",
    "type": "originphp-plugin",
    "keywords": [
        "originphp",
        "code generation"
    ],
    "homepage": "https://www.originphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamiel Sharief",
            "email": "js@originphp.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Generate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Generate\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2.0",
        "originphp/framework": "2.x",
        "originphp/plugin-installer": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "minimum-stability": "dev"
}
